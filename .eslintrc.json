{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],

  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": false,
        "singleQuote": true
      }
    ],
    "react/jsx-key": "error",
    "no-duplicate-imports": "error",
    "no-unused-vars": "warn"
